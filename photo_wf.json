{
  "name": "Unified Photo Pipeline",
  "nodes": [
    {
      "parameters": {
        "url": "http://rapidraw-comfyui:1420/api/process",
        "method": "POST",
        "jsonParameters": true,
        "json": {
          "image": "={{$binary.data}}",
          "preset": "professional",
          "output_format": "comfyui"
        }
      },
      "name": "Process RAW",
      "type": "n8n-nodes-base.httpRequest"
    },
    {
      "parameters": {
        "url": "http://rapidraw-comfyui:8188/api/prompt",
        "method": "POST",
        "jsonParameters": true,
        "json": {
          "prompt": {
            "workflow": "enhance_photo",
            "input_image": "={{$json.output_path}}"
          }
        }
      },
      "name": "AI Enhancement",
      "type": "n8n-nodes-base.httpRequest"
    }
  ]
}
